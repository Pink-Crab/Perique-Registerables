{
    "name": "pinkcrab/registerables",
    "type": "library",
    "description": "Abstract classes for creating post types, taxonomies, metaboxes and ajax calls.",
    "keywords": [],
    "homepage": "https://pinkcrab.co.uk",
    "license": "MIT",
    "authors": [{
        "name": "Glynn Quelch",
        "email": "glynn.quelch@pinkcrab.co.uk",
        "homepage": "http://clappo.co.uk",
        "role": "Developer"
    }],
    "autoload": {
        "psr-4": {
            "PinkCrab\\Registerables\\": "src"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "PinkCrab\\Registerables\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": " ^8.5",
        "phpstan/phpstan": "1.*",
        "szepeviktor/phpstan-wordpress": "<=1.1.7",
        "php-stubs/wordpress-stubs": "6.2-",
        "roots/wordpress": "6.2.*",
        "wp-phpunit/wp-phpunit": "6.2.*",
        "dealerdirect/phpcodesniffer-composer-installer": "<=1.0.0",
        "wp-coding-standards/wpcs": "<=2.3.0",
        "yoast/phpunit-polyfills": "^1.0.0",
        "symfony/var-dumper": "<=6.2.7",
        "gin0115/wpunit-helpers": "1.1.*",
        "vlucas/phpdotenv": "<=5.5.0",
        "pinkcrab/wp-rest-schema": "0.0.*"
    },
    "require": {
        "php": ">=7.4.0",
        "pinkcrab/perique-framework-core": "2.0.*"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --coverage-clover clover.xml --testdox --colors=always",
        "coverage": "./vendor/bin/phpunit --coverage-html coverage-report --testdox --colors=always",
        "analyse": "./vendor/bin/phpstan analyse src -l8",
        "sniff": "./vendor/bin/phpcs src/ -v",
        "all": "composer test && composer analyse && composer sniff"
    },
    "config": {
        "allow-plugins": {
            "roots/wordpress-core-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}